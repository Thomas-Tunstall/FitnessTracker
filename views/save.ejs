<div class="container">
    <h1 class="text-center">Join Journey Now</h1>
    <form action="/login" method="POST" class="registration-form">
        <label>
            <span class="label-text">Email</span>
            <input type="text" name="email">
        </label>
        <label class="password">
            <span class="label-text">Password</span>
            <button class="toggle-visibility" title="toggle password visibility" tabindex="-1">
                <span class="glyphicon glyphicon-eye-close"></span>
            </button>
            <input type="password" name="password">
        </label>
        <label class="col-one-half">
            <span class="label-text">Weight</span>
            <input type="text" name="weight">
        </label>
        <label class="col-one-half">
            <span class="label-text">Age</span>
            <input type="text" name="age">
        </label>
        <label class="col-one-half">
            <span class="label-text">Height</span>
            <input type="text" name="height">
        </label>
        <label class="col-one-half">
            <span class="label-text">Gender</span>
            <select id="gender" name="gender">
                <option value="Female">Female</option>
                <option value="Male">Male</option>
            </select>
        </label>

        <div class="text-center">
            <button class="submit" type="submit">Join Now</button>
        </div>
    </form>
    <hr>
    <div class="text-center">
        <span class="label-text">Already A Journey Member ?</span>
        <br>
        <br>
        <form action="/login">
            <button class="submit" type="submit">Login Here</button>

    </div>
</div>

